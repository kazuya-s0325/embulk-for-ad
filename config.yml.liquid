in:
  type: sqlserver
  driver_path: C:\drivers\mssql-jdbc-12.8.1.jre8.jar
  host: 192.168.xxx.xxx
  user: user_name
  password: password
  instance: MSSQLSERVER
  database: database
{% include 'query/query' %}

{% include 'filter/filter' %}

out:
  type: file
  path_prefix: C:\outputPath
  file_ext: csv
  formatter:
    type: csv
    delimiter: ","
    newline: CRLF
    newline_in_field: LF
    charset: SHIFT_JIS
    quote_policy: MINIMAL
    quote: '"'
    null_string: ""
